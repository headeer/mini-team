        { "_type": "block", "listItem": "bullet", "markDefs": [], "children": [{ "_type": "span", "text": "Gęste korzenie i pnie do 25–30 cm", "marks": [] }] },
        { "_type": "block", "listItem": "bullet", "markDefs": [], "children": [{ "_type": "span", "text": "Zmarznięta ziemia (późna jesień / zima)", "marks": [] }] },
